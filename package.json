		"url": "https://github.com/SerenniaHan/obsidian-formatted-paster-plugin.git"
	},
	"homepage": "https://github.com/SerenniaHan/obsidian-formatted-paster-plugin",
	"bugs": {
		"url": "https://github.com/SerenniaHan/obsidian-formatted-paster-plugin/issues"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
